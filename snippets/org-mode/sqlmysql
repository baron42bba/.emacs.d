# -*- mode: snippet -*-
# name: sqlmysql
# expand-env: ((yas-indent-line 'fixed))
# key: sqlmysql
# --
:PROPERTIES:
:engine: mysql
:dbhost: ${1:localhost}
:dbuser: ${2:user}
:dbpassword: ${3:(concat bba-db)}
:database: ${4:mysql}
:cmdline: --protocol=tcp
:exports: both
:END:

(setq bba-db "$5")$0

