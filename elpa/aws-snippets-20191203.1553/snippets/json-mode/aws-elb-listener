{
   "InstancePort" : "${1:-}",
   "InstanceProtocol" : "${2:$$(yas-choose-value '("HTTP" "HTTPS" "TCP" "SSL"))}",
   "LoadBalancerPort" : "${3:-}",
   "Protocol" : "${4:$$(yas-choose-value '("HTTP" "HTTPS" "TCP" "SSL"))}"
}$0